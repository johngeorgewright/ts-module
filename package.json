    "@commitlint/types": "19.8.0",
    "@types/bun": "latest",
    "husky": "9.1.7",
    "lint-staged": "15.5.1",
    "nodemon": "3.1.10",
    "prettier": "3.5.3",
    "rimraf": "6.0.1",
